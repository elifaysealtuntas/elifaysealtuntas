#include <stdio.h>
int main()
{
   
int arr1[3][3]={{3,2,1,},{8,8,8,},{5,1,}};
int arr2[3][3]={1,2,3,4,5,6,7,8,};
int i,j;
printf("Values in array1 by row are:\n");
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        printf("%d",arr1[i][j]);
    }
    printf("\n");
    
}
printf("Values in array2 by row are:\n");
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        printf("%d",arr2[i][j]);
    }
    printf("\n");
    
}
